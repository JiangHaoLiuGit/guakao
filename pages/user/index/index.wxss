
.margin{
	margin-left: 30rpx;
	margin-right: 30rpx;
}
.top{
	box-sizing: border-box;
	background: url("data:image/png;base64,");
	
	background-color:#2AC980 ;
	background-repeat: no-repeat;
	background-size: contain;
	position: relative;
	overflow: hidden;
}

.top .userImg{
	width: 120rpx;
	height: 120rpx;
	margin-right: 46rpx;
	border-radius: 50%;
	z-index: 2;
	border-radius: 50%;
	overflow: hidden;
	margin:0 auto;
}
.top .userImg image{
	width: 120rpx;
	height: 120rpx;
	
}
.top .userInfo{
	display: flow-root;
	z-index: 2;
}
.top .userInfo .userName{
	font-size: 32rpx;
	line-height: 42rpx;
	color: #fff;
	margin: 20rpx 0 8rpx;
	text-align: center;
}
.top .userInfo .userFirm{
	font-size: 24rpx;
	line-height: 32rpx;
	color: #fff;
	text-align: center;
	margin-bottom: 100rpx;
}
.not{
	background: #2E95FA;
}
.success{
	background: #2AC980;
}
.blue{
	background: #2AC3C9;
}
.fail{
	background: #FC5E4B;
}
.notB{
	border-color: #2E95FA!important;
	color: #2E95FA!important;
}
.successB{
	border-color: #2AC980!important;
	color: #2AC980!important;
}
.failB{
	border-color: #FC5E4B!important;
	color: #FC5E4B!important;
}
.yellow{
	background: #FEAA3C;
}
.center{
	min-height: 101rpx;
	background: transparent;
	display:flex;
	justify-content: space-between;
	position: relative;
	bottom: 60rpx;
}
.center .content{
	
}
.center .content .block{
	width: 200rpx;
	height: 200rpx;
	display: flex;
	flex-direction: column;
	align-items: center;
	justify-content: center;
	border-radius: 8rpx;
}
.center .content .block image{
	width: 100rpx;
	height: 100rpx;
}
.center .content .block.first image{
	width: 110rpx;
	height: 110rpx;
}
.center .content p{
	margin-top: 12rpx;
	font-size: 28rpx;
	line-height: 38rpx;
	color: #fff;
}
.bottom{
	margin-top: 16rpx;
}
.bottom>p.title{
	font-size: 36rpx;
	line-height: 48rpx;
	color: #333;
	font-weight: bold;
}
.bottom .box{
	height: calc(100vh - (48rpx + 16rpx + 250rpx + 352rpx + 50rpx + 30rpx));
	margin-top: 30rpx;
	padding: 0 30rpx 0;
	overflow-y: scroll;
}
.point{
	z-index: 101;
}
.bottom .point .box{
	height: 77vh;
}
.bottom .box .block>text{
	color: #9a9a9a;
    display: block;
    width: 100%;
    /* text-align: center; */
    margin-top: 20rpx;
}
.bottom .box .block>.cont{
	margin-top: 40rpx;
	padding: 20rpx 38rpx 24rpx 18rpx;
	box-shadow: 0 6px 12px rgba(0,0,0,.08);
}

.bottom .box .block>.cont:first-child{
	margin-top: 0;
}
.bottom .box .block>.cont .firmImg{
	font-family: 'PingFang SC';
	font-weight: bold;
	font-size: 52rpx;
	line-height: 28rpx;
	width: 74rpx;
	height: 74rpx;
	border-radius: 50%;
	border:2rpx solid;
	display: flex;
	flex-direction: column;
	align-items: center;
	justify-content: center;
	margin-top: 12rpx;
}
.bottom .box .block>.cont .firmImg .font{
}
.bottom .box .block>.cont .firmImg image{
	width: 44rpx;
	height: 44rpx;
}
.bottom .box .block>.cont .firmInfo{
	padding-left: 18rpx;
	width: 545rpx;
}
.bottom .box .block>.cont .firmInfo .firmName{
	position: relative;
	padding-bottom: 10rpx;
	border-bottom:2rpx solid #F2F2F2;
}
.bottom .box .block>.cont .firmInfo .firmName .name{
	font-size: 32rpx;
	line-height: 42rpx;
	color: #333;
	width: 450rpx;
	overflow:hidden;
	white-space:nowrap;
	text-overflow:ellipsis;
}
.bottom .box .block>.cont .firmInfo .firmName .staus{
	position: absolute;
	right: -38rpx;
	top: 0;
	width: 86rpx;
	height: 40rpx;
	border-top-left-radius:20rpx;
	border-bottom-left-radius:20rpx;
	text-align: center;
	line-height: 40rpx;
	font-size: 24rpx;
	display: block;
	color: #fff;
}
.bottom .box .block>.cont .firmInfo .firmPro{
	color: #666;
	font-size: 24rpx;
	line-height: 32rpx;
}
.blockBox{
  width: 750rpx;
  height: 100vh;
  background: rgba(0,0,0,.5);
  z-index: 100;
  position: absolute;
  left: 0;
  top: 0;
}
.blockBox .por{
  width: 750rpx;
  height: 100vh;
}
.blockBox .child{
  bottom: 0;
  left: 0;
  border-top-left-radius: 40rpx;
  border-top-right-radius: 40rpx;
  background: #fff;
  width: 750rpx;
  padding: 18rpx 30rpx 36rpx 30rpx ;
}
.blockBox .child .title{
  display: inline-block;
  width: 100%;
  color: #2AC980;
  font-size: 36rpx;
  line-height: 48rpx;
  margin-bottom: 22rpx;
  text-align: center;
}
.blockBox .child .tit{
  padding-bottom: 10rpx;
  border-bottom: 2rpx solid #E3E3E3;
  color: #333333;
  font-size: 28rpx;
  line-height: 38rpx;
}
.blockBox .child .tit textarea{
  width: 690rpx;
  min-height: 160rpx;
  border: 2rpx solid #E3E3E3;
  border-radius: 8rpx;
  padding: 24rpx;
  margin-top: 16rpx;
  margin-bottom: 10rpx;
}
.blockBox .child .input{
  margin-top: 36rpx;
}
.blockBox .child .input text{
	display:block;
	margin-bottom: 16rpx;
}
.blockBox .child .input input{
  height: 80rpx;
  border: 2rpx solid #E3E3E3;
  border-radius: 8rpx;
  padding-left: 20rpx;
}
.blockBox .child .check{
  padding: 0 40rpx;
  margin-top: 24rpx;
}
.blockBox .child .check .group{
  display: flex;
  justify-content: space-between;
}
.blockBox .child .check label{
  color: #333333;
  font-size: 28rpx;
  line-height: 38rpx;
}
.blockBox .child .check label .checkbox{
  width: 36rpx;
  height: 36rpx;
  margin-right: 20rpx;
}
.blockBox .child button{
  height: 92rpx;
  width: 100%;
  border-radius: 8rpx;
  background: #2AC980;
  line-height: 92rpx;
  font-size: 40rpx;
  margin-top: 48rpx;
  color: #fff;
}
.blockBox .por.por3{
  width: 750rpx;
  height: 100vh;
  display: flex;
  justify-content: center;
  align-items: center;
}
.blockBox .por.por3 image{
  max-width: 700rpx;
  max-height: 90vh;
}